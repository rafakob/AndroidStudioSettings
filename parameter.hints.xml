<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="Java" />
      <blacklist language="Groovy" />
      <blacklist language="Kotlin">
        <added pattern="com.nhaarman.mockito_kotlin.times(numInvocations)" />
        <added pattern="android.annotation.SuppressLint(value)" />
        <added pattern="retrofit2.http.POST(value)" />
        <added pattern="com.mikepenz.fastadapter.commons.adapters.FastItemAdapter.notifyAdapterItemChanged(position)" />
        <added pattern="com.rafakob.synomanager.entity.SystemUsage(cpuUsage,ramUsage,networkIncoming,networkOutcoming)" />
        <added pattern="com.mikepenz.fastadapter.commons.adapters.FastItemAdapter.add(position,item)" />
        <added pattern="com.dazn.gherkin.annotation.Gherkin(feature,shouldStartActivity)" />
      </blacklist>
    </blacklists>
    <option id="SHOW_FUNCTION_HINT" value="true" />
  </component>
</application>